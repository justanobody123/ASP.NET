@page
@using ToDoWebApplication.Models
@model ToDoWebApplication.Pages.TasksModel
@{
}


@foreach (UserTask task in Model.TaskService.GetTasks())
{
	<div class="card mb-3">
		<div class="card-body">
			<h5 class="card-title">@task.Title</h5>
			<p class="card-text">@task.Description</p>
		</div>
	</div>
}